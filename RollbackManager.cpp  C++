#include "RollbackManager.h"
RollbackManager::RollbackManager() : top(-1) {}
void RollbackManager::record(int r, int s, int z, RequestState st) {
    if(top < 99) history[++top] = {r, s, z, st};
}
bool RollbackManager::isEmpty() { return top == -1; }
RollbackAction RollbackManager::pop() { return history[top--]; }